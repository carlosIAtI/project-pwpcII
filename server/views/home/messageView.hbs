<div class="row">
    <div class="col s12 m10 offset-m1 l8 offset-l2">
        <div class="card">
            <div class="card-image">
                <img src="https://user-images.githubusercontent.com/42922067/173722197-3c679c43-0d19-4824-beb5-d0abb9ae1157.svg"
                    alt="blue print image" />
                <h4 class="teal-text text-darken-3">Mensajes</h4>
                <ul>
                    {{#each messages}}
                    <div class="card-panel">
                        <span class="back-text ">Nombre: {{ this.name }}</span>
                        <br>
                        <span class="back-text ">Email: {{ this.email }}</span>
                        <br>
                        <span class="teal-text text-darken-3">{{ this.mensaje }}</span>

                    </div>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>